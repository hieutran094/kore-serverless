{
  "name": "basic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf .aws-sam",
    "develop": "node -r @swc-node/register ./src/local.ts",
    "dev": "nodemon -L",
    "dev:sam": "sam local start-api --env-vars json/env.json",
    "build": "webpack-cli",
    "build:swc": "swc --ignore ./src/views/* ./src -d build  --config-file .swcrc",
    "watch": "webpack-cli -w",
    "test": "jest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --check \"**/*.ts\""
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.223",
    "@swc/jest": "^0.2.22",
    "@types/aws-lambda": "^8.10.101",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "aws-sam-webpack-plugin": "^0.12.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^6.7.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.10.1",
    "aws-sdk": "^2.1189.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "uuid": "^8.3.2"
  }
}
