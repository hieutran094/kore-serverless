{
  "name": "basic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "develop": "node -r @swc-node/register ./src/local.ts",
    "dev": "nodemon -L",
    "azure": "sls deploy list",
    "dev:sls": "cross-env NODE_ENV=development sls offline start --stage offline",
    "deploy:dev": "sls deploy --stage dev",
    "test": "jest",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\"",
    "lint:fix": "eslint --fix \"{src,apps,libs,tests}/**/*.ts\"",
    "format": "prettier --check \"**/*.ts\""
  },
  "lint-staged": {
    "*.ts": [
      "yarn prettier --write",
      "yarn eslint --fix"
    ]
  },
  "devDependencies": {
    "@azure/functions": "^3.2.0",
    "@swc-node/register": "^1.5.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.223",
    "@swc/jest": "^0.2.22",
    "@types/aws-lambda": "^8.10.101",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.6.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^6.7.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "serverless": "^3.21.0",
    "serverless-azure-functions": "^2.1.3",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^9.2.0",
    "serverless-webpack": "^5.3.2",
    "supertest": "^6.2.4",
    "swc-loader": "^0.2.3",
    "typescript": "^4.7.4",
    "webpack": "^4.43.0"
  },
  "dependencies": {
    "azure-aws-serverless-express": "^0.1.5",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "uuid": "^8.3.2"
  }
}