{
  "disabled": false,
  "bindings": [
    {
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route": "{*segments}",
      "authLevel": "anonymous",
      "methods": [
        "GET",
        "POST",
        "OPTIONS"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "entryPoint": "handler",
  "scriptFile": "../src/handler.js"
}